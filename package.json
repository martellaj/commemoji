{
  "name": "commemoji",
  "version": "1.1.4",
  "description": "A command line tool to make emojify your boring commit messages.",
  "bin": {
    "commemoji": "./bin/source/app.js"
  },
  "scripts": {
    "undo": "git reset --soft HEAD~",
    "lint": "semistandard | snazzy || true",
    "test": "jasmine",
    "build": "rm -rf bin && tsc",
    "postinstall": "typings install && npm run build"
  },
  "main": "app.js",
  "author": "Joe Martella",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "emojilib": "^1.1.0"
  },
  "devDependencies": {
    "jasmine": "^2.4.1",
    "typescript": "^1.7.5"
  }
}
